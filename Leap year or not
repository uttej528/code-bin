#include <stdio.h>

int main()
{int year;
 printf("enter number to check whether it is leap year or not");
 scanf("%d",&year);
 if((year%4==0&&year%100!=0)||(year%400==0))
 {
     printf("entered year is leap year");
    
 }else{
     printf("entered year is not a leap year");
 }
  

    return 0;
}
