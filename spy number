#include<stdio.h>
int main(){
    int n,rem,sum=0,pro=1;
    printf("enter the value of n");
    scanf("%d",&n);
    
    while(n!=0){
        rem=n%10;
        sum=sum+rem;
         pro=pro*rem;
        n=n/10;
     }
    
    while(n!=0){
        rem=n%10;
        sum=sum+rem;
        n=n/10;
        
    }
    
    if(pro==sum){
        
        
        printf("it is a spy number");
    }else{
        
        printf("it is not a spy number");
    }
return 0;
    
}
