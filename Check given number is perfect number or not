#include <stdio.h>

int main()
{
  int n,i;int sum=0;
  printf("enter the value of n");
  scanf("%d",&n);
  for(i=1;i<=n/2;i++)
  if(n%i==0){
      
      printf("%d\n",i);
      
      sum=sum+i;
      
  }
 if(sum==n){
     
     printf("entered number is perfect number");
 }else{
     printf("it is not a perfect number");
     
 }

    return 0;
}
