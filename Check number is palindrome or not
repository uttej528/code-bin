#include <stdio.h>

int main()
{
    int n,rem,k; int sum=0;
    printf("enter the value of n");
    scanf("%d",&n);
    k=n;
    while(n!=0){
    rem=n%10;
    sum=sum*10+rem;
    n=n/10;
    }
    printf("%d",sum);
  if(k==sum){
      
      printf("it is a palindrome number");
  }else{
      printf("it is not a palindrome number");
  }
    return 0;
}
