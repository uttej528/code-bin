// Online C compiler to run C program online
#include <stdio.h>

int main() {
int i,n,count=0;
printf("enter the value of n");
scanf("%d",&n);
for(i=1;i<n;i++)
{ if(n%i==0){
    count=count+1;
}
}
if(count<2)
{
printf("value is prime");  
}else
{
    printf("it is not a prime");
}
    
    return 0;
}
