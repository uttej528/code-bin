#include <stdio.h>

int main()
{
  int n,i,rem,k;int sum=0;
  
  printf("enter the value of n");
  scanf("%d",&n);
  k=n;
  while(n>0){
  rem=n%10;
  sum=sum+(rem*rem*rem); 
  n=n/10;
}
printf("%d",sum);

if(k==sum)
{
    printf("its a armstrong number");
}else{
    printf("its not a armstrong number");
}
  

    return 0;
}
