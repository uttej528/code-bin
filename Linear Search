#include <stdio.h>
int main()
{ int a[100],i,size,key,count=0;
  printf("enter the size of array");
  scanf("%d",&size);
  printf("enter the elements of array");
  for(i=0;i<size;i++)
  {
      scanf("%d",&a[i]);
  }
  printf("enter the key to search in the array");
  scanf("%d",&key);
   for(i=0;i<size;i++)
  {
      if(key==a[i]){
          printf("key element found");
          break;
      }
    count=count+1;  
  }
  if(count==size){
      
      printf("key element not found");
  }
    
    return 0;
}
