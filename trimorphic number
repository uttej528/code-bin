#include<stdio.h>
int main(){
    int n,rem,pro=1;
    printf("enter the value of n");
    scanf("%d",&n);
    rem=n%10;
    pro=rem*rem*rem;
        
        if(pro==n){
            printf("it is a trimorphic number");
        }else{
            printf("it is not a trimorphic number");
        }
        
        
        
        
      return 0;  
        
        
    }
